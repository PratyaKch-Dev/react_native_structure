{
  "name": "react_structure",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean-app": "yarn clean-npm && yarn clean-pod && yarn clean-ios && yarn build:ios && yarn clean-gradlew && yarn xprebuild:android",
    "clean-gradlew": "cd android && ./gradlew clean && rm -rf ~/.gradle/caches/*",
    "clean-npm": "rm -rf node_modules/ && yarn",
    "clean-pod": "cd ios && rm -rf Podfile.lock && pod install",
    "clean-ios": "xcodebuild clean -workspace ios/react_structure.xcworkspace -scheme react_structure",
    "prebuild:android": "yarn xprebuild:android && cd ./android && rm -rf .idea && rm -rf react_structure.iml && cd ../",
    "xprebuild:android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && rm -rf android/app/src/main/res/drawable-mdpi/src_*  && rm -rf android/app/src/main/res/drawable-mdpi/node_modules_* && rm -rf android/app/src/main/res/drawable-hdpi/src_*  && rm -rf android/app/src/main/res/drawable-hdpi/node_modules_* && rm -rf android/app/src/main/res/drawable-ldpi/src_*  && rm -rf android/app/src/main/res/drawable-ldpi/node_modules_* && rm -rf android/app/src/main/res/drawable-xhdpi && rm -rf android/app/src/main/res/drawable-xxhdpi && rm -rf android/app/src/main/res/drawable-xxxhdpi && rm -rf android/app/src/main/res/raw && rm -rf android/app/build/generated/res/resValues/release/values",
    "android-build-release": "cd ./android && ./gradlew clean && ./gradlew bundleRelease",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "build:android": "yarn prebuild:android && cd ./android && ./gradlew clean && ./gradlew bundleRelease",
    "build:android-apk": "yarn prebuild:android  && cd ./android && ./gradlew clean && ./gradlew assembleRelease",
    "archive-ios": "xcodebuild archive -workspace ios/react_structure.xcworkspace -scheme react_structure -archivePath ~/Documents/react_structure.xcarchive",
    "detox-build-ios": "detox build -c ios.sim.debug -a e2e",
    "detox-build-android": "detox build -c android.emu.debug -a e2e",
    "detox-android": "detox test -c android.emu.debug -a e2e",
    "detox-ios": "detox test -c ios.sim.debug -a e2e",
    "codepush": "node ./react_structure-command.js -c",
    "prettier": "prettier --write ./src",
    "prt": "prettier --write ./src"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.9.0",
    "@react-native-community/async-storage": "^1.12.1",
    "axios": "^0.21.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "base-64": "^1.0.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-exit-app": "^1.1.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-navigation": "7.12.0",
    "react-native-orientation-locker": "^1.3.1",
    "react-native-paper": "^4.8.1",
    "react-native-quick-actions": "^0.3.13",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-toast-message": "^1.4.9",
    "react-native-unimodules": "^0.13.3",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.3.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assests/font/"
    ]
  }
}
